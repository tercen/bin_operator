{
  "name": "Bin",
  "description": "Defines intervals based on values' range and bin data points.",
  "tags": [
    "preprocessing"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/bin_operator"
  ],
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "method",
      "defaultValue": "Equal Width",
      "values": [
        "Equal Width",
        "Quantiles",
        "Pretty"
      ],
      "description": "Method to define bins: either Equal width, Quantiles, or Pretty."
    },
    {
      "kind": "DoubleProperty",
      "name": "n_bins",
      "defaultValue": 10,
      "values": {},
      "description": "A number indicating the number of desired bins."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Row",
            "type": "",
            "description": "Optional row factor",
            "ontologyMapping": "row",
            "crosstabMapping": "row",
            "cardinality": "0..n",
            "factors": []
          },
          {
            "kind": "MetaFactor",
            "name": "Column",
            "type": "",
            "description": "Optional column factor",
            "ontologyMapping": "column",
            "crosstabMapping": "column",
            "cardinality": "0..n",
            "factors": []
          }
        ],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "y-axis",
                "type": "numeric",
                "description": "numeric, measurement, per cell",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}